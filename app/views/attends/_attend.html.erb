<% if @month_counts >= 20 %>
  <div class="attend-perfect">
    <h3>Perfect!!</h3>
  </div>
    <h4>Month <%=@month_counts%> 回</h4>
    <h4>Year <%=@year_counts%> 回</h4>
<%elsif @month_counts >= 15 %>
  <div class="attend-excellent">
    <h3>Excellent！</h3>
  </div>
    <h4>Month <%=@month_counts%> 回</h4>
    <h4>Year <%=@year_counts%> 回</h4>
<%elsif @month_counts >= 10 %>
  <div class="attend-great">
    <h3>Great！</h3>
  </div>
    <h4>Month <%=@month_counts%> 回</h4>
    <h4>Year <%=@year_counts%> 回</h4>
<%elsif @month_counts >= 5%>
    <h3>その調子！</h3>
    <h4>Month <%=@month_counts%> 回</h4>
    <h4>Year <%=@year_counts%> 回</h4>
<%elsif @month_counts == 1%>
    <h3>今月の目標を考えよう</h3>
    <h4>Month <%=@month_counts%> 回</h4>
    <h4>Year <%=@year_counts%> 回</h4>
<%else%>
    <h3>いいね!</h3>
    <h4>Month <%=@month_counts%> 回</h4>
    <h4>Year <%=@year_counts%> 回</h4>
<%end%>